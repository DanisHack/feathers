{
  "name": "@adhawk/flux-vision",
  "description": "Track End of Funnel Behavior in Shopify Liquid templates through Segment.io",
  "version": "1.0.5",
  "author": "developers@tryadhawk.com",
  "license": "MIT",
  "main": "./lib/flux-vision.cjs.js",
  "module": "./lib/flux-vision.esm.js",
  "unpkg": "./lib/flux-vision.umd.min.js",
  "typings": "./lib/index.d.ts",
  "keywords": [
    "segment",
    "shopify",
    "liquid template",
    "analytics.js"
  ],
  "repository": {
    "type": "git",
    "url": "@adhawk/feathers"
  },
  "sideEffects": true,
  "scripts": {
    "clean": "rm -Rf ./lib/* ./meta/bundlesize/* ./meta/coverage/* ./.rpt2_cache",
    "prepare": "npm run build",
    "prebuild": "npm run clean",
    "build": "tsc -p .",
    "postbuild": "rollup -c",
    "watch": "tsc-watch --onSuccess \"npm run postbuild\" -p .",
    "predeploy": "npm run build",
    "deploy": "npm publish",
    "test": "jest --config ../../config/jest.config.js --testPathPattern packages/flux-vision",
    "test:watch": "jest --config ../../config/jest.config.js --testPathPattern packages/flux-vision --watch",
    "test:cjs": "npm run build && jest --config ../../config/jest.cjs.config.js --testPathPattern packages/flux-vision",
    "test:umd": "npm run build && jest --config ../../config/jest.umd.config.js --testPathPattern packages/flux-vision"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "jest-environment-jsdom": "^25.1.0",
    "typescript": "^3.1.3"
  }
}
